{
  "manifest_version": 3,
  "name": "LJW's Extension",
  "description": "The first extension that LJW made.",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icon32.png",
    "default_title": "Click me!"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/inject.js"]
    }
  ]
}
